      },
      {
        "name": "blocked_words",
        "required": true,
        "type": "array",
        "raw_type": "string[]",
        "description": "List of words that you want to check."
      }
    ],
    "examples": [
      {
        "title": "Example",
        "curl": "curl --get -H \"Access-Token:xxx\" \\\n--data-urlencode \"advertiser_id=ADVERTISER_ID\" \\\n--data-urlencode \"blocked_words=[\\\"BLOCKED_WORDS\\\"]\" \\\nhttps://business-api.tiktok.com/open_api/v1.3/blockedword/check/"
      }
    ]
  },
  "response": {
    "example": {
