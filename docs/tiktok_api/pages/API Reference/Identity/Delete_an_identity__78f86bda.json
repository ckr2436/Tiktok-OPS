{
  "title": "Delete an identity",
  "endpoint": "https://business-api.tiktok.com/open_api/v1.3/identity/delete/",
  "method": "POST",
  "category_path": [
    "Identity"
  ],
  "official_only": true,
  "request": {
    "headers": [
      {
        "name": "Access-Token",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "Authorized access token. For details, see Authentication ."
      },
      {
        "name": "Content-Type",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "The content type of the request. Allowed value: \"application/json\" ."
      }
    ],
    "parameters": [
      {
        "name": "advertiser_id",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "Advertiser ID."
      },
      {
        "name": "identity_id",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "Identity ID."
      },
      {
        "name": "identity_type",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "Identity type. Currently, this field only supports CUSTOMIZED_USER ."
      }
    ],
    "examples": [
      {
        "title": "Example",
        "curl": "curl --location  --request POST 'https://business-api.tiktok.com/open_api/v1.3/identity/delete/' \\\n--header 'Access-Token: {{Access-Token}}' \\\n--header 'Content-Type: application/json' \\\n--data '{\n    \"advertiser_id\":\"{{advertiser_id}}\",\n    \"identity_id\":\"{{identity_id}}\",\n    \"identity_type\":\"CUSTOMIZED_USER\"\n}'"
      }
    ]
  },
  "response": {
    "example": {
      "status_line": "HTTPS/1.1 200 OK",
      "body": "{\n    \"code\": 0,\n    \"message\": \"OK\",\n    \"request_id\": \"{{request_id}}\",\n    \"data\": {}\n}"
    }
  }
}