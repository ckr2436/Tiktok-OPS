{
  "title": "Get music authorization info of a video",
  "endpoint": "https://business-api.tiktok.com/open_api/v1.3/identity/music/authorization/",
  "method": "GET",
  "category_path": [
    "Identity"
  ],
  "official_only": true,
  "request": {
    "headers": [
      {
        "name": "Access-Token",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "Authorized access token. For details, see Authentication ."
      }
    ],
    "parameters": [
      {
        "name": "advertiser_id",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "Ad account ID."
      },
      {
        "name": "item_id",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "Tiktok post ID."
      },
      {
        "name": "identity_id",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "Identity ID."
      },
      {
        "name": "identity_type",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "Identity type. Enum values: AUTH_CODE (Authorized Post User), TT_USER (TikTok Business Account User), BC_AUTH_TT (TheTikTok account that a Business Center is authorized to access). See Identities for details."
      },
      {
        "name": "identity_authorized_bc_id Conditional",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "ID of the Business Center that a TikTok Account User in Business Center identity is associated with. Required when identity_type is BC_AUTH_TT ."
      },
      {
        "name": "locations",
        "required": true,
        "type": "array",
        "raw_type": "string[]",
        "description": "List of targeting locations. You must use the same set of locations as when you create the ad."
      },
      {
        "name": "start_time",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "Ad deployment start time (UTC), in the format of \"YYYY-MM-DD HH:MM:SS\"."
      },
      {
        "name": "end_time",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "Ad deployment end time (UTC), in the format of \"YYYY-MM-DD HH:MM:SS\"."
      }
    ],
    "examples": [
      {
        "title": "Example",
        "curl": "curl --get -H \"Access-Token:xxx\" \\\n--data-urlencode \"advertiser_id=ADVERTISER_ID\" \\\n--data-urlencode \"item_id=ITEM_ID\" \\\n--data-urlencode \"identity_id=IDENTITY_ID\" \\\n--data-urlencode \"identity_type=IDENTITY_TYPE\" \\\n--data-urlencode \"identity_authorized_bc_id=IDENTITY_AUTHORIZED_BC_ID\" \\\n--data-urlencode \"locations=[\\\"LOCATIONS\\\"]\" \\\n--data-urlencode \"start_time=START_TIME\" \\\n--data-urlencode \"end_time=END_TIME\" \\\nhttps://business-api.tiktok.com/open_api/v1.3/identity/music/authorization/"
      }
    ]
  },
  "response": {
    "example": {
      "status_line": "HTTPS/1.1 200 OK",
      "body": "{\n    \"code\": 0,\n    \"message\": \"OK\",\n    \"request_id\": \"202208101819310102510031480D2B9AA4\",\n    \"data\": {\n        \"music_authorization\": [\n            {\n                \"music_status\": MUSIC_STATUS,\n                \"item_id\": \"ITEM_ID\",\n                \"authorization_infos\": [\n                    {\n                        \"title\": \"TITLE\",\n                        \"composer\":\" COMPOSER\",\n                        \"author\": \"AUTHOR,\n                        \"publisher\": PUBLISHER,\n                        \"lyricist\": \"LYRICIST\",\n                        \"labels\": [\"LABELS\"],\n                        \"authorization_type\": \"AUTHORIZED\",\n                        \"music_id\": \"MUSIC_ID\"\n                    }\n                ]\n            }\n        ]\n    }\n}"
    }
  }
}