      }
    ],
    "examples": [
      {
        "title": "Example",
        "curl": "curl -H \"Access-Token:xxx\" -H \"Content-Type:application/json\" -X POST \\\n-d '{\n    \"advertiser_id\": \"ADVERTISER_ID\", \n    \"playable_id\": \"PLAYABLE_ID\"\n}' \\\nhttps://business-api.tiktok.com/open_api/v1.3/playable/delete/"
