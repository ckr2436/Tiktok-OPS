{
  "title": "Get TTO Creator Marketplace campaigns",
  "endpoint": "https://business-api.tiktok.com/open_api/v1.3/tto/tcm/campaign/",
  "method": "GET",
  "category_path": [
    "TikTok One"
  ],
  "official_only": true,
  "request": {
    "headers": [
      {
        "name": "Access-Token",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "Access token authorized by a TikTok One Creator Marketplace account. For details, see Authentication ."
      }
    ],
    "parameters": [
      {
        "name": "tto_tcm_account_id",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "The ID of the TikTok One Creator Marketplace account. To obtain the IDs of the accounts that you can access via Access-Token , use /tto/oauth2/tcm/ ."
      },
      {
        "name": "campaign_ids",
        "required": false,
        "type": "array",
        "raw_type": "string[]",
        "description": "The ID list of TikTok One Creator Marketplace campaigns. Max size: 5. To create a campaign and obtain the campaign ID, use /tto/tcm/campaign/create/ ."
      },
      {
        "name": "campaign_type",
        "required": false,
        "type": "string",
        "raw_type": "string",
        "description": "The type of the TTO Creator Marketplace campaign. Enum value: CAMPAIGN : A campaign with a campaign-level invite link . BRAND_LINK : A campaign with a brand-level invite link . Default value: CAMPAIGN ."
      },
      {
        "name": "page",
        "required": false,
        "type": "integer",
        "raw_type": "integer",
        "description": "Current page number. Value range: â‰¥ 1. Default value: 1."
      },
      {
        "name": "page_size",
        "required": false,
        "type": "integer",
        "raw_type": "integer",
        "description": "Page size. Value range: 1-5. Default value: 5."
      }
    ],
    "examples": [
      {
        "title": "Example",
        "curl": "curl --location --request GET 'https://business-api.tiktok.com/open_api/v1.3/tto/tcm/campaign/?tto_tcm_account_id={{tto_tcm_account_id}}&campaign_ids=[\"{{campaign_id}}\",\"{{campaign_id}}\"]' \\\n--header 'Access-Token: {{Access-Token}}' \\"
      }
    ]
  },
  "response": {
    "example": {
      "status_line": "HTTPS/1.1 200 OK",
      "body": "{\n    \"code\": 0,\n    \"message\": \"OK\",\n    \"request_id\": \"{{request_id}}\",\n    \"data\": {\n        \"campaigns\": [\n            {\n                \"campaign_type\": \"CAMPAIGN\",\n                \"brand_name\": \"{{brand_name}}\",\n                \"campaign_description\": \"{{campaign_description}}\",\n                \"anchor_id\": null,\n                \"campaign_id\": \"{{campaign_id}}\",\n                \"campaign_name\": \"{{campaign_name}}\",\n                \"country_codes\": [\n                    \"US\"\n                ],\n                \"create_time\": \"2024-10-03 19:32:04\",\n                \"handle_names\": [\n                    \"{{handle_name}}\"\n                ],\n                \"invite_link\": \"https://www.tiktok.com/t/1111/\",\n                \"spark_ads_requested_authorization_days\": 0,\n                \"video_ids\": []\n            },\n            {\n                \"campaign_type\": \"CAMPAIGN\",\n                \"brand_name\": \"{{brand_name}}\",\n                \"campaign_description\": \"{{campaign_description}}\",\n                \"campaign_id\": \"{{campaign_id}}\",\n                \"campaign_name\": \"{{campaign_name}}\",\n                \"country_codes\": [\n                    \"US\"\n                ],\n                \"create_time\": \"2024-09-25 20:47:49\",\n                \"handle_names\": [\n                    \"{{handle_name}}\"\n                ],\n                \"invite_link\": \"https://www.tiktok.com/t/333333/\",\n                \"spark_ads_requested_authorization_days\": 0,\n                \"advertiser_ids\":[],\n                \"video_ids\": [\n                    \"{{video_id}}\"\n                ]\n            }\n        ],\n        \"page_info\": {\n            \"page\": 1,\n            \"page_size\": 5,\n            \"total_number\": 2,\n            \"total_page\": 1\n        }\n    }\n}"
    }
  }
}