{
  "title": "Check TTO Creator Status",
  "endpoint": "https://business-api.tiktok.com/open_api/v1.3/tto/tcm/creator/status/get/",
  "method": "GET",
  "category_path": [
    "TikTok One"
  ],
  "official_only": true,
  "request": {
    "headers": [
      {
        "name": "Access-Token",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "Access token authorized by TTO Creator Marketplace accounts. For details, see Authentication ."
      }
    ],
    "parameters": [
      {
        "name": "tto_tcm_account_id",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "The ID of the TTO Creator Marketplace account. To obtain the IDs of the accounts that you can access via Access-Token , use /tto/oauth2/tcm/ ."
      },
      {
        "name": "handle_names",
        "required": true,
        "type": "array",
        "raw_type": "string[]",
        "description": "The handle names of TikTok Creators. Max size: 20."
      }
    ],
    "examples": [
      {
        "title": "Example",
        "curl": "curl --location --request GET 'https://business-api.tiktok.com/open_api/v1.3/tto/tcm/creator/status/get/?tto_tcm_account_id={{tto_tcm_account_id}}&handle_names=[\"{{handle_name}}\",\"{{handle_name}}\",\"{{handle_name}}\"]' \\\n--header 'Access-Token: {{Access-Token}}'"
      }
    ]
  },
  "response": {
    "example": {
      "status_line": "HTTPS/1.1 200 OK",
      "body": "{\n    \"code\": 0,\n    \"message\": \"OK\",\n    \"request_id\": \"{{request_id}}\",\n    \"data\": {\n        \"onboarding_status\": [\n            {\n                \"handle_name\": \"{{handle_name}}\",\n                \"status\": \"NOT_IN\"\n            },\n            {\n                \"handle_name\": \"{{handle_name}}\",\n                \"status\": \"IN\"\n            },\n            {\n                \"handle_name\": \"{{handle_name}}\",\n                \"status\": \"INVALID\"\n            }\n        ]\n    }\n}"
    }
  }
}