{
  "title": "Get the details of a TTO Creator Marketplace account",
  "endpoint": "https://business-api.tiktok.com/open_api/v1.3/tto/oauth2/info/",
  "method": "GET",
  "category_path": [
    "TikTok One"
  ],
  "official_only": true,
  "request": {
    "headers": [
      {
        "name": "Access-Token",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "Access token authorized by a TTO Creator Marketplace account. For details, see Authentication ."
      }
    ],
    "parameters": [
      {
        "name": "tto_tcm_account_id",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "The ID of the TTO Creator Marketplace account. To obtain the IDs of the accounts that you can access via Access-Token , use /tto/oauth2/tcm/ ."
      }
    ],
    "examples": [
      {
        "title": "Example",
        "curl": "curl --location  --request GET 'https://business-api.tiktok.com/open_api/v1.3/tto/oauth2/info/?tto_tcm_account_id={{tto_tcm_account_id}}' \\\n--header 'Access-Token: {{Access-Token}}'"
      }
    ]
  },
  "response": {
    "example": {
      "status_line": "HTTPS/1.1 200 OK",
      "body": "{\n    \"code\": 0,\n    \"message\": \"OK\",\n    \"request_id\": \"{{request_id}}\",\n    \"data\": {\n        \"account_name\": \"{{account_name}}\",\n        \"business_verification_status\": \"PASSED\",\n        \"country\": \"US\",\n        \"timezone\": \"America/Los_Angeles\",\n        \"tto_tcm_account_id\": \"{{tto_tcm_account_id}}\"\n    }\n}"
    }
  }
}