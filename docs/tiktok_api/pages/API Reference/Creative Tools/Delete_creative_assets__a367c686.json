{
  "title": "Delete creative assets",
  "endpoint": "https://business-api.tiktok.com/open_api/v1.3/creative/asset/delete/",
  "method": "POST",
  "category_path": [
    "Creative Tools"
  ],
  "official_only": true,
  "request": {
    "headers": [
      {
        "name": "Access-Token",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "Authorized access token. For details, see Authentication ."
      },
      {
        "name": "Content-Type",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "The content type of the request. Allowed value: \"application/json\" ."
      }
    ],
    "parameters": [
      {
        "name": "advertiser_id",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "Advertiser ID."
      },
      {
        "name": "video_ids",
        "required": false,
        "type": "array",
        "raw_type": "string[]",
        "description": "A list of video IDs. At most 50 IDs can be included in the list."
      },
      {
        "name": "image_ids",
        "required": false,
        "type": "array",
        "raw_type": "string[]",
        "description": "A list of image IDs. At most 50 IDs can be included in the list."
      }
    ],
    "examples": [
      {
        "title": "Examples",
        "curl": "curl --location --request POST 'https://business-api.tiktok.com/open_api/v1.3/creative/asset/delete/' \\\n--header 'Access-Token: {{Access-Token}}' \\\n--header 'Content-Type: application/json' \\\n--data-raw '{\n    \"advertiser_id\": \"{{advertiser_id}}\",\n    \"video_ids\": \"{{video_ids}}\",\n    \"image_ids\": \"{{image_ids}}\"\n}'"
      }
    ]
  },
  "response": {
    "example": {
      "status_line": "HTTPS/1.1 200 OK",
      "body": "{\n  \"message\": \"OK\",\n  \"code\": 0,\n  \"data\": {\n          “failed_video_ids”: [],\n          “failed_image_ids”: []\n        },\n  \"request_id\": \"{{request_id}}\"\n}"
    }
  }
}