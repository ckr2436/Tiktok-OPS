        "curl": "curl --location --request GET 'https://business-api.tiktok.com/open_api/v1.3/creative_fatigue/get/' \\\n--header 'Content-Type: application/json' \\\n--data '{\n    \"advertiser_id\": \"{{advertiser_id}}\",\n    \"ad_id\": \"{{ad_id}}\",\n    \"filtering\": {\n        \"start_date\": \"2023-05-22\",\n        \"end_date\": \"2023-05-24\"\n    },\n    \"page\": 1,\n    \"page_size\": 500\n}'"
