{
  "title": "Get Terms",
  "endpoint": "https://business-api.tiktok.com/open_api/v1.3/term/get/",
  "method": "GET",
  "category_path": [
    "Terms"
  ],
  "official_only": true,
  "request": {
    "headers": [
      {
        "name": "Access-Token",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "Authorized access token. For details, see Authentication ."
      }
    ],
    "parameters": [
      {
        "name": "advertiser_id",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "Advertiser ID"
      },
      {
        "name": "lang",
        "required": false,
        "type": "string",
        "raw_type": "string",
        "description": "The language of the agreement. Enum values: EN , JA , ZH Default value: EN ."
      },
      {
        "name": "term_type",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "Type of agreement. Enum values: LeadAds ."
      }
    ],
    "examples": [
      {
        "title": "Example",
        "curl": "curl --get -H \"Access-Token:xxx\" \\\n--data-urlencode \"advertiser_id=< advertiser_id >\" \\\n--data-urlencode \"lang=< lang >\" \\\n--data-urlencode \"term_type=< term_type >\" \\\nhttps://business-api.tiktok.com/open_api/v1.3/term/get/"
      }
    ]
  },
  "response": {
