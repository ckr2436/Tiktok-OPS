{
  "title": "Get popular tracks from the Commercial Music Library",
  "endpoint": "https://business-api.tiktok.com/open_api/v1.3/discovery/cml/trending_list/",
  "method": "GET",
  "category_path": [
    "Discovery"
  ],
  "official_only": true,
  "request": {
    "headers": [
      {
        "name": "Access-Token",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "Access token authorized by TikTok creators. To obtain an access token, use /tt_user/oauth2/token/ ."
      }
    ],
    "parameters": [
      {
        "name": "business_id",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "Application specific unique identifier for the TikTok account. Pass the value of the open_id field returned in the response of /tt_user/oauth2/token/ to this field."
      },
      {
        "name": "genre",
        "required": false,
        "type": "string",
        "raw_type": "string",
        "description": "The specific genre to filter the results by. Default value: ALL . For enum values, see List of values for genre ."
      },
      {
        "name": "country_code",
        "required": false,
        "type": "string",
        "raw_type": "string",
        "description": "The code of the location to filter the results by. Default value: US . See Location code for the supported values."
      },
      {
        "name": "date_range",
        "required": false,
        "type": "string",
        "raw_type": "string",
        "description": "The timeframe in which the tracks have been popular. Enum values: 1DAY : last one day. 7DAY : last seven days. 30DAY : last 30 days. 90DAY : last 90 days. Default value: 7DAY ."
      }
    ],
    "examples": [
      {
        "title": "Example",
        "curl": "curl --location  --request GET 'https://business-api.tiktok.com/open_api/v1.3/discovery/cml/trending_list/?business_id={{business_id}}&genre=FOLK&country_code=US&date_range=7DAY' \\\n--header 'Access-Token: {{Access-Token}}'"
      }
    ]
  },
  "response": {
    "example": {
      "status_line": "HTTPS/1.1 200 OK",
      "body": "{\n    \"code\": 0,\n    \"message\": \"OK\",\n    \"request_id\": \"{{request_id}}\",\n    \"data\": {\n        \"list\": [\n            {\n                \"artist\": \"{{artist}}\",\n                \"commercial_music_id\": \"{{commercial_music_id}}\",\n                \"commercial_music_name\": \"{{commercial_music_name}}\",\n                \"duration\": 123,\n                \"genres\": [\n                    \"Folk\"\n                ],\n                \"preview_url\": \"{{preview_url}}\",\n                \"rank_position\": \"1\",\n                \"thumbnail_url\": \"{{thumbnail_url}}\",\n                \"trending_history\": [\n                    {\n                        \"date\": \"2024-09-24\",\n                        \"rank_position_daily\": \"1\"\n                    },\n                    {\n                        \"date\": \"2024-09-23\",\n                        \"rank_position_daily\": \"1\"\n                    },\n                    {\n                        \"date\": \"2024-09-22\",\n                        \"rank_position_daily\": \"1\"\n                    },\n                    {\n                        \"date\": \"2024-09-21\",\n                        \"rank_position_daily\": \"1\"\n                    },\n                    {\n                        \"date\": \"2024-09-20\",\n                        \"rank_position_daily\": \"1\"\n                    },\n                    {\n                        \"date\": \"2024-09-19\",\n                        \"rank_position_daily\": \"1\"\n                    },\n                    {\n                        \"date\": \"2024-09-18\",\n                        \"rank_position_daily\": \"1\"\n                    }\n                ]\n            },\n            ...,\n            {\n                \"artist\": \"{{artist}}\",\n                \"commercial_music_id\": \"{{commercial_music_id}}\",\n                \"commercial_music_name\": \"{{commercial_music_name}}\",\n                \"duration\": 148,\n                \"genres\": [\n                    \"Folk\",\n                    \"Indie_Folk\"\n                ],\n                \"preview_url\": \"{{preview_url}}\",\n                \"rank_position\": \"3\",\n                \"thumbnail_url\": \"{{thumbnail_url}}\",\n                \"trending_history\": [\n                    {\n                        \"date\": \"2024-09-24\",\n                        \"rank_position_daily\": \"3\"\n                    },\n                    {\n                        \"date\": \"2024-09-23\",\n                        \"rank_position_daily\": \"2\"\n                    },\n                    {\n                        \"date\": \"2024-09-22\",\n                        \"rank_position_daily\": \"2\"\n                    },\n                    {\n                        \"date\": \"2024-09-21\",\n                        \"rank_position_daily\": \"2\"\n                    },\n                    {\n                        \"date\": \"2024-09-20\",\n                        \"rank_position_daily\": \"2\"\n                    },\n                    {\n                        \"date\": \"2024-09-19\",\n                        \"rank_position_daily\": \"3\"\n                    },\n                    {\n                        \"date\": \"2024-09-18\",\n                        \"rank_position_daily\": \"3\"\n                    }\n                ]\n            }\n        ]\n    }\n}"
    }
  }
}