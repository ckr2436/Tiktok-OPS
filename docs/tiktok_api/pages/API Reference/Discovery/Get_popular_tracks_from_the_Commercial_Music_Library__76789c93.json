{
  "title": "Get popular tracks from the Commercial Music Library",
  "endpoint": "https://business-api.tiktok.com/open_api/v1.3/discovery/cml/trending_list/",
  "method": "GET",
  "category_path": [
    "Discovery"
  ],
  "official_only": true,
  "request": {
    "headers": [
      {
        "name": "Access-Token",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "Access token authorized by TikTok creators. To obtain an access token, use /tt_user/oauth2/token/ ."
      }
    ],
    "parameters": [
      {
        "name": "business_id",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "Application specific unique identifier for the TikTok account. Pass the value of the open_id field returned in the response of /tt_user/oauth2/token/ to this field."
      },
      {
        "name": "genre",
        "required": false,
        "type": "string",
        "raw_type": "string",
        "description": "The specific genre to filter the results by. Default value: ALL . For enum values, see List of values for genre ."
      },
      {
        "name": "country_code",
        "required": false,
        "type": "string",
        "raw_type": "string",
        "description": "The code of the location to filter the results by. Default value: US . See Location code for the supported values."
      },
      {
        "name": "date_range",
        "required": false,
        "type": "string",
        "raw_type": "string",
        "description": "The timeframe in which the tracks have been popular. Enum values: 1DAY : last one day. 7DAY : last seven days. 30DAY : last 30 days. 90DAY : last 90 days. Default value: 7DAY ."
      }
    ],
    "examples": [
      {
        "title": "Example",
        "curl": "curl --location  --request GET 'https://business-api.tiktok.com/open_api/v1.3/discovery/cml/trending_list/?business_id={{business_id}}&genre=FOLK&country_code=US&date_range=7DAY' \\\n--header 'Access-Token: {{Access-Token}}'"
      }
    ]
  },
  "response": {
    "top_fields": [
      {
        "name": "code",
        "required": false,
        "type": "number",
        "raw_type": "number",
        "description": "Response code. For the complete list of response codes and descriptions, see Appendix-Return Code ."
      },
      {
        "name": "message",
        "required": false,
        "type": "string",
        "raw_type": "string",
        "description": "Response message. For details, see Appendix - Return Codes ."
      },
      {
        "name": "request_id",
        "required": false,
        "type": "string",
        "raw_type": "string",
        "description": "The log ID of a request, which uniquely identifies the request."
      },
      {
        "name": "data",
        "required": false,
        "type": "object",
        "raw_type": "object",
        "description": "Returned data."
      }
    ],
    "data": {
      "fields": [
        {
          "name": "list",
          "required": false,
          "type": "array",
          "raw_type": "object[]",
          "description": "The list of the trending tracks."
        },
        {
          "name": "commercial_music_id",
          "required": false,
          "type": "string",
          "raw_type": "string",
          "description": "The ID of the track. Example: 1234567890 ."
        },
        {
          "name": "commercial_music_name",
          "required": false,
          "type": "string",
          "raw_type": "string",
          "description": "The name of the track."
        },
        {
          "name": "duration",
          "required": false,
          "type": "integer",
          "raw_type": "integer",
          "description": "The duration of the track in seconds. Note : For a track that is only a part (clips) of the full piece of music and not the entire piece, the duration might be recorded as 0."
        },
        {
          "name": "thumbnail_url",
          "required": false,
          "type": "string",
          "raw_type": "string",
          "description": "The URL of the thumbnail image associated with the track."
        },
        {
          "name": "artist",
          "required": false,
          "type": "string",
          "raw_type": "string",
          "description": "The name of the artist who performed the track."
        },
        {
          "name": "preview_url",
          "required": false,
          "type": "string",
          "raw_type": "string",
          "description": "The URL for a preview of the track. Validity period: six hours."
        },
        {
          "name": "genres",
          "required": false,
          "type": "array",
          "raw_type": "string[]",
          "description": "A list of genres associated with the track."
        },
        {
          "name": "rank_position",
          "required": false,
          "type": "string",
          "raw_type": "string",
          "description": "Current rank position of the track in the selected region ( country_code ). Example: 100 ."
        },
        {
          "name": "trending_history",
          "required": false,
          "type": "array",
          "raw_type": "object[]",
          "description": "The historical ranking data for the track, spanning up to the past 30 days."
        },
        {
          "name": "date",
          "required": false,
          "type": "string",
          "raw_type": "string",
          "description": "Date, in the format of YYYY-MM-DD . Example: 2024-01-01 ."
        },
        {
          "name": "rank_position_daily",
          "required": false,
          "type": "string",
          "raw_type": "string",
          "description": "The rank position of the track in the selected region ( country_code ) on the date. Example: 73 . Note : If the track does not perform well compared to the last time period, the track will not be considered a popular track and the value of this field will be NULL ."
        }
      ],
      "list": {
        "item_fields": []
      }
    },
    "other_tables": [],
    "example": {
      "status_line": "HTTPS/1.1 200 OK",
      "body": "{\n    \"code\": 0,\n    \"message\": \"OK\",\n    \"request_id\": \"{{request_id}}\",\n    \"data\": {\n        \"list\": [\n            {\n                \"artist\": \"{{artist}}\",\n                \"commercial_music_id\": \"{{commercial_music_id}}\",\n                \"commercial_music_name\": \"{{commercial_music_name}}\",\n                \"duration\": 123,\n                \"genres\": [\n                    \"Folk\"\n                ],\n                \"preview_url\": \"{{preview_url}}\",\n                \"rank_position\": \"1\",\n                \"thumbnail_url\": \"{{thumbnail_url}}\",\n                \"trending_history\": [\n                    {\n                        \"date\": \"2024-09-24\",\n                        \"rank_position_daily\": \"1\"\n                    },\n                    {\n                        \"date\": \"2024-09-23\",\n                        \"rank_position_daily\": \"1\"\n                    },\n                    {\n                        \"date\": \"2024-09-22\",\n                        \"rank_position_daily\": \"1\"\n                    },\n                    {\n                        \"date\": \"2024-09-21\",\n                        \"rank_position_daily\": \"1\"\n                    },\n                    {\n                        \"date\": \"2024-09-20\",\n                        \"rank_position_daily\": \"1\"\n                    },\n                    {\n                        \"date\": \"2024-09-19\",\n                        \"rank_position_daily\": \"1\"\n                    },\n                    {\n                        \"date\": \"2024-09-18\",\n                        \"rank_position_daily\": \"1\"\n                    }\n                ]\n            },\n            ...,\n            {\n                \"artist\": \"{{artist}}\",\n                \"commercial_music_id\": \"{{commercial_music_id}}\",\n                \"commercial_music_name\": \"{{commercial_music_name}}\",\n                \"duration\": 148,\n                \"genres\": [\n                    \"Folk\",\n                    \"Indie_Folk\"\n                ],\n                \"preview_url\": \"{{preview_url}}\",\n                \"rank_position\": \"3\",\n                \"thumbnail_url\": \"{{thumbnail_url}}\",\n                \"trending_history\": [\n                    {\n                        \"date\": \"2024-09-24\",\n                        \"rank_position_daily\": \"3\"\n                    },\n                    {\n                        \"date\": \"2024-09-23\",\n                        \"rank_position_daily\": \"2\"\n                    },\n                    {\n                        \"date\": \"2024-09-22\",\n                        \"rank_position_daily\": \"2\"\n                    },\n                    {\n                        \"date\": \"2024-09-21\",\n                        \"rank_position_daily\": \"2\"\n                    },\n                    {\n                        \"date\": \"2024-09-20\",\n                        \"rank_position_daily\": \"2\"\n                    },\n                    {\n                        \"date\": \"2024-09-19\",\n                        \"rank_position_daily\": \"3\"\n                    },\n                    {\n                        \"date\": \"2024-09-18\",\n                        \"rank_position_daily\": \"3\"\n                    }\n                ]\n            }\n        ]\n    }\n}"
    }
  }
}
