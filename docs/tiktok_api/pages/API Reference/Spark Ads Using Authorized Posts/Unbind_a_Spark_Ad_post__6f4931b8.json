{
  "title": "Unbind a Spark Ad post",
  "endpoint": "https://business-api.tiktok.com/open_api/v1.3/tt_video/unbind/",
  "method": "POST",
  "category_path": [
    "Spark Ads Using Authorized Posts"
  ],
  "official_only": true,
  "request": {
    "headers": [
      {
        "name": "Access-Token",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "Authorized access token. For details, see Authentication ."
      },
      {
        "name": "Content-Type",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "The content type of the request. Allowed value: \"application/json\""
      }
    ],
    "parameters": [
      {
        "name": "advertiser_id",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "The advertiser ID"
      },
      {
        "name": "item_id",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "The ID of the Spark Ads post"
      }
    ],
    "examples": [
      {
        "title": "Example",
        "curl": "curl -H \"Access-Token:xxx\" -H \"Content-Type:application/json\" -X POST \\\n-d '{\n    \"advertiser_id\": \"ADVERTISER_ID\", \n    \"item_id\": \"ITEM_ID\"\n}' \\\nhttps://business-api.tiktok.com/open_api/v1.3/tt_video/unbind/"
      }
    ]
  },
  "response": {
    "example": {
      "status_line": "HTTPS/1.1 200 OK",
      "body": {
        "message": "OK",
        "code": 0,
        "data": {}
      }
    }
  }
}