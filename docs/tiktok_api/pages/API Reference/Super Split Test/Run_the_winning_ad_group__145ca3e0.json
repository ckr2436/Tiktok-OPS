{
  "title": "Run the winning ad group",
  "endpoint": "https://business-api.tiktok.com/open_api/v1.3/split_test/promote/",
  "method": "POST",
  "category_path": [
    "Super Split Test"
  ],
  "official_only": true,
  "request": {
    "headers": [
      {
        "name": "Access-Token",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "Authorized access token. For details, see Authentication ."
      }
    ],
    "parameters": [
      {
        "name": "advertiser_id",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "Advertiser ID."
      },
      {
        "name": "split_test_group_id",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "Split test group ID."
      },
      {
        "name": "winning_object_id",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "The winning ad group ID. You can use /split_test/result/get/ to get metric and its corresponding p_value . There will be a winning ad group when P value <=0.1. Note : Do not specify this field for campaign level split test. Otherwise, an error will occur."
      }
    ],
    "examples": [
      {
        "title": "Example",
        "curl": "curl --location --request POST https://business-api.tiktok.com/open_api/v1.3/split_test/promote/' \\\n--header 'Content-Type: application/json' \\\n--header 'Access-Token: xxx' \\\n--data-raw '{\n    \"advertiser_id\": \"xx\",\n    \"split_test_group_id\": \"xx\",\n    \"winning_object_id\": \"xx\"\n}'"
      }
    ]
  },
  "response": {
    "example": {
      "status_line": "",
      "body": {
        "code": 0,
        "message": "OK",
        "request_id": "xxx",
        "data": {}
      }
    }
  }
}