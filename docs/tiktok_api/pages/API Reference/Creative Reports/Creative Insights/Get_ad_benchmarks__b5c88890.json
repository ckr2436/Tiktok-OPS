{
  "title": "Get ad benchmarks",
  "endpoint": "https://business-api.tiktok.com/open_api/v1.3/report/ad_benchmark/get/",
  "method": "GET",
  "category_path": [
    "Creative Reports",
    "Creative Insights"
  ],
  "official_only": true,
  "request": {
    "headers": [
      {
        "name": "Access-Token",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "Authorized access token. For details, see Authentication ."
      }
    ],
    "parameters": [
      {
        "name": "advertiser_id",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "Advertiser ID"
      },
      {
        "name": "compare_time_window",
        "required": false,
        "type": "string",
        "raw_type": "string",
        "description": "Time window for comparison (in days). Enum: 7 (default), 14 , 30 , 60"
      },
      {
        "name": "dimensions",
        "required": true,
        "type": "array",
        "raw_type": "string[]",
        "description": "List of dimensions to compare. 1-4 dimensions are allowed. Enum: LOCATION , AD_CATEGORY , EXTERNAL_ACTION , PLACEMENT"
      },
      {
        "name": "metrics_fields",
        "required": false,
        "type": "array",
        "raw_type": "string[]",
        "description": "Metrics that you want to get. By default, all metrics will be returned. See the Supported metrics section for supported metrics."
      },
      {
        "name": "filtering",
        "required": true,
        "type": "object",
        "raw_type": "object",
        "description": "Filtering conditions. You must specify one and only one out of the three conditions allowed"
      },
      {
        "name": "ad_ids",
        "required": false,
        "type": "array",
        "raw_type": "string[]",
        "description": "IDs of the ads that you want to get benchmark results for"
      },
      {
        "name": "adgroup_ids",
        "required": false,
        "type": "array",
        "raw_type": "string[]",
        "description": "IDs of the ad groups that you want to get benchmark results for"
      },
      {
        "name": "campaign_ids",
        "required": false,
        "type": "array",
        "raw_type": "string[]",
        "description": "IDs of the campaigns that you want to get benchmark results for"
      },
      {
        "name": "sort_field",
        "required": false,
        "type": "string",
        "raw_type": "string",
        "description": "Field to sort by. By default, results will be sorted by CREATE_TIME"
      },
      {
        "name": "sort_type",
        "required": false,
        "type": "string",
        "raw_type": "string",
        "description": "Sorting type. Enum: ASC (starting from the one that was created earliest), DES (starting from the one that was created latest)"
      },
      {
        "name": "page",
        "required": false,
        "type": "number",
        "raw_type": "number",
        "description": "Page number"
      },
      {
        "name": "page_size",
        "required": false,
        "type": "number",
        "raw_type": "number",
        "description": "Page size"
      }
    ],
    "examples": []
  },
