{
  "title": "Migrate leads to a BC",
  "endpoint": "https://business-api.tiktok.com/open_api/v1.3/page/library/transfer/",
  "method": "POST",
  "category_path": [
    "Leads"
  ],
  "official_only": true,
  "request": {
    "headers": [
      {
        "name": "Access-Token",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "Authorized access token. For details, see Authentication ."
      },
      {
        "name": "Content-Type",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "The content type of the request. Allowed value: \"application/json\""
      }
    ],
    "parameters": [
      {
        "name": "advertiser_id",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "ID of the advertiser that owns the leads."
      },
      {
        "name": "bc_id",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "ID of the Business Center that you want to migrate leads to."
      }
    ],
    "examples": [
      {
        "title": "Example",
        "curl": "curl -H \"Access-Token:xxx\" -H \"Content-Type:application/json\" -X POST \\\n-d '{\n    \"advertiser_id\": \"ADVERTISER_ID\",\n    \"bc_id\": \"BC_ID\"\n}' \\\nhttps://business-api.tiktok.com/open_api/v1.3/page/library/transfer/"
      }
    ]
  },
  "response": {
    "example": {
      "status_line": "HTTPS/1.1 200 OK",
      "body": {
        "message": "OK",
        "code": 0,
        "data": {}
      }
    }
  }
}