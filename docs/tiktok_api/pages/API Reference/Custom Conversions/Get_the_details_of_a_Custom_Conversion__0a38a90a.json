{
  "title": "Get the details of a Custom Conversion",
  "endpoint": "https://business-api.tiktok.com/open_api/v1.3/custom_conversion/get/",
  "method": "GET",
  "category_path": [
    "Custom Conversions"
  ],
  "official_only": true,
  "request": {
    "headers": [
      {
        "name": "Access-Token",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "Authorized access token. For details, see Authentication ."
      }
    ],
    "parameters": [
      {
        "name": "advertiser_id",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "Advertiser ID."
      },
      {
        "name": "custom_conversion_id",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "The ID of the Custom Conversion. To obtain the list of Custom Conversions associated with a Pixel or an App, use /custom_conversion/list/ ."
      },
      {
        "name": "event_source_type",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "Event source type. Enum values: PIXEL : Pixel. APP : App."
      },
      {
        "name": "event_source_id",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "Event source ID. When event_source_type is PIXEL , specify a Pixel ID. To obtain IDs of Pixels within your ad account, use /pixel/list/ and check the returned pixel_id . When event_source_type is APP , specify the app platform ID of a Self Attribution Network (SAN) App. To obtain app platform IDs of SAN Apps within your ad account, use /app/list/ . Confirm that the returned self_attribution_enabled for the App is true and check the returned app_platform_id ."
      }
    ],
    "examples": [
      {
        "title": "Example",
        "curl": "curl --location --request GET 'https://business-api.tiktok.com/open_api/v1.3/custom_conversion/get/?advertiser_id={{advertiser_id}}&event_source_type=PIXEL&event_source_id={{event_source_id}}&custom_conversion_id={{custom_conversion_id}}' \\\n--header 'Access-token: {{Access-token}}'"
      }
    ]
  },
  "response": {
    "example": {
      "status_line": "HTTPS/1.1 200 OK",
      "body": "{\n    \"code\": 0,\n    \"message\": \"OK\",\n    \"request_id\": \"{{request_id}}\",\n    \"data\": {\n        \"activity_status\": \"WAITING_FOR_ACTIVITY\",\n        \"create_time\": \"{{create_time}}\",\n        \"custom_conversion_id\": \"{{custom_conversion_id}}\",\n        \"description\": \"{{description}}\",\n        \"event_source_id\": \"{{event_source_id}}\",\n        \"event_source_type\": \"PIXEL\",\n        \"name\": \"{{name}}\",\n        \"optimization_event\": \"SHOPPING\",\n        \"rules\": [\n            {\n                \"operator\": \"NOT_CONTAIN\",\n                \"parameter\": \"EVENT_ID\",\n                \"values\": [\n                    \"{{event_id}}\",\n                    \"{{event_id}}\"\n                ]\n            },\n            {\n                \"operator\": \"START_WITH\",\n                \"parameter\": \"DESCRIPTION\",\n                \"values\": [\n                    \"{{description_value}}\"\n                ]\n            }\n        ],\n        \"update_time\": \"{{update_time}}\"\n    }\n}"
    }
  }
}