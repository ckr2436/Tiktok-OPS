{
  "title": "Get Custom Conversions associated with an event source",
  "endpoint": "https://business-api.tiktok.com/open_api/v1.3/custom_conversion/list/",
  "method": "GET",
  "category_path": [
    "Custom Conversions"
  ],
  "official_only": true,
  "request": {
    "headers": [
      {
        "name": "Access-Token",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "Authorized access token. For details, see Authentication ."
      }
    ],
    "parameters": [
      {
        "name": "advertiser_id",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "Advertiser ID."
      },
      {
        "name": "event_source_type",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "Event source type. Enum values: PIXEL : Pixel. APP : App."
      },
      {
        "name": "event_source_id",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "Event source ID. When event_source_type is PIXEL , specify a Pixel ID. To obtain IDs of Pixels within your ad account, use /pixel/list/ and check the returned pixel_id . When event_source_type is APP , specify the app platform ID of a Self Attribution Network (SAN) App. To obtain app platform IDs of SAN Apps within your ad account, use /app/list/ . Confirm that the returned self_attribution_enabled for the App is true and check the returned app_platform_id ."
      },
      {
        "name": "search_keyword",
        "required": false,
        "type": "string",
        "raw_type": "string",
        "description": "The keyword to search for Custom Conversions by. You can search by Custom Conversion name or Custom Conversion ID. When you search by Custom Conversion name, fuzzy matches are supported. When you search by Custom Conversion ID, exact matches are supported."
      },
      {
        "name": "sort_field",
        "required": false,
        "type": "string",
        "raw_type": "string",
        "description": "Sorting field. Enum values: CREATE_TIME : The time when the Custom Conversion was created. TOTAL_COUNT : The total conversion count for the Custom Conversion. Default value: CREATE_TIME ."
      },
      {
        "name": "sort_type",
        "required": false,
        "type": "string",
        "raw_type": "string",
        "description": "Sorting type. Enum values: DESC : descending. ASC : ascending. Default value: DESC ."
      },
      {
        "name": "page",
        "required": false,
        "type": "number",
        "raw_type": "number",
        "description": "Current page number. Value range: â‰¥ 1. Default value: 1."
      },
      {
        "name": "page_size",
        "required": false,
        "type": "number",
        "raw_type": "number",
        "description": "Page size. Value range: 1-1,000. Default value: 10."
      }
    ],
    "examples": [
      {
        "title": "Example",
        "curl": "curl --location --request GET 'https://business-api.tiktok.com/open_api/v1.3/custom_conversion/list/?advertiser_id={{advertiser_id}}&event_source_type=PIXEL&event_source_id={{event_source_id}}&page=1&page_size=1000' \\\n--header 'Access-token: {{Access-token}}'"
      }
    ]
  },
  "response": {
    "example": {
      "status_line": "HTTPS/1.1 200 OK",
      "body": "{\n    \"code\": 0,\n    \"message\": \"OK\",\n    \"request_id\": \"{{request_id}}\",\n    \"data\": {\n        \"list\": [\n            {\n                \"activity_info\": {\n                    \"activity_status\": \"WAITING_FOR_ACTIVITY\",\n                    \"total_count\": 0\n                },\n                \"create_time\": \"{{create_time}}\",\n                \"custom_conversion_id\": \"{{custom_conversion_id}}\",\n                \"description\": \"{{description}}\",\n                \"event_source_id\": \"{{event_source_id}}\",\n                \"event_source_type\": \"PIXEL\",\n                \"name\": \"{{name}}\",\n                \"optimization_event\": \"SHOPPING\",\n                \"rules\": [\n                    {\n                        \"operator\": \"NOT_CONTAIN\",\n                        \"parameter\": \"EVENT_ID\",\n                        \"values\": [\n                            \"{{event_id}}\",\n                            \"{{event_id}}\"\n                        ]\n                    },\n                    {\n                        \"operator\": \"START_WITH\",\n                        \"parameter\": \"DESCRIPTION\",\n                        \"values\": [\n                            \"{{description_value}}\"\n                        ]\n                    }\n                ],\n                \"update_time\": \"{{update_time}}\"\n            },\n            {\n                \"activity_info\": {\n                    \"activity_status\": \"NO_RECENT_ACTIVITY\",\n                    \"last_record_time\": \"{{last_record_time}}\",\n                    \"total_count\": 1\n                },\n                \"create_time\": \"{{create_time}}\",\n                \"custom_conversion_id\": \"{{custom_conversion_id}}\",\n                \"description\": \"\",\n                \"event_source_id\": \"{{event_source_id}}\",\n                \"event_source_type\": \"PIXEL\",\n                \"name\": \"{{name}}\",\n                \"optimization_event\": \"ON_WEB_CART\",\n                \"rules\": [\n                    {\n                        \"operator\": \"CONTAIN\",\n                        \"parameter\": \"CONTENT_ID\",\n                        \"values\": [\n                            \"{{content_id}}\"\n                        ]\n                    }\n                ],\n                \"update_time\": \"{{update_time}}\"\n            },\n            ...\n            {\n                \"activity_info\": {\n                    \"activity_status\": \"ACTIVE\",\n                    \"last_record_time\": \"{{last_record_time}}\",\n                    \"total_count\": 20\n                },\n                \"create_time\": \"{{create_time}}\",\n                \"custom_conversion_id\": \"{{custom_conversion_id}}\",\n                \"description\": \"\",\n                \"event_source_id\": \"{{event_source_id}}\",\n                \"event_source_type\": \"PIXEL\",\n                \"name\": \"{{name}}\",\n                \"optimization_event\": \"SHOPPING\",\n                \"rules\": [\n                    {\n                        \"operator\": \"CONTAIN\",\n                        \"parameter\": \"STATUS\",\n                        \"values\": [\n                            \"{{status_value}}\"\n                        ]\n                    }\n                ],\n                \"update_time\": \"{{update_time}}\"\n            }\n        ],\n        \"page_info\": {\n            \"page\": 1,\n            \"page_size\": 1000,\n            \"total_number\": 98,\n            \"total_page\": 1\n        }\n    }\n}"
    }
  }
}