{
  "title": "Update the statuses of ads",
  "endpoint": "https://business-api.tiktok.com/open_api/v1.3/ad/status/update/",
  "method": "POST",
  "category_path": [
    "Ads"
  ],
  "official_only": true,
  "request": {
    "headers": [
      {
        "name": "Access-Token",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "Authorized access token. For details, see Authentication ."
      },
      {
        "name": "Content-Type",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "Request message type. Allowed format: \"application/json\" ."
      }
    ],
    "parameters": [
      {
        "name": "ad_ids Conditional",
        "required": false,
        "type": "array",
        "raw_type": "string[]",
        "description": "A list of ad IDs. Allowed quantity: 1-20 . Either ad_ids or aco_ad_ids has to be set."
      },
      {
        "name": "aco_ad_ids Conditional",
        "required": false,
        "type": "array",
        "raw_type": "string[]",
        "description": "A list of ACO ad IDs. Only support ENABLE and DISABLE for ACO ads. Allowed quantity: 1-20 . Either ad_ids or aco_ad_ids has to be set."
      },
      {
        "name": "advertiser_id",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "Advertiser ID"
      },
      {
        "name": "operation_status",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "The operation being made. Enum values: DELETE (delete), DISABLE (pause), ENABLE (enable). Note : The status of deleted ads or ads in deleted campaigns or ad groups cannot be modified."
      }
    ],
    "examples": [
      {
        "title": "Example",
        "curl": "curl -H \"Access-Token:xxx\" -H \"Content-Type:application/json\" -X POST \\\n-d '{\n    \"advertiser_id\": \"ADVERTISER_ID\",\n    \"operation_status\": \"OPT_STATUS\",\n    \"ad_ids\": [\n        \"AD_IDS\"\n    ]\n}' \\\nhttps://business-api.tiktok.com/open_api/v1.3/ad/status/update/"
      }
    ]
  },
  "response": {
    "example": {
      "status_line": "HTTPS/1.1 200 OK",
      "body": {
        "message": "OK",
        "code": 0,
        "data": {
          "status": "ENABLE",
          "ad_ids": [
            11,
            22
          ]
        },
        "request_id": "2019091810032301011023224213552"
      }
    }
  }
}