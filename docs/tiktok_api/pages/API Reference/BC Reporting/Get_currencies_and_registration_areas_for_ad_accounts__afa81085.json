{
  "title": "Get currencies and registration areas for ad accounts",
  "endpoint": "https://business-api.tiktok.com/open_api/v1.3/bc/advertiser/attribute/",
  "method": "GET",
  "category_path": [
    "BC Reporting"
  ],
  "official_only": true,
  "request": {
    "headers": [
      {
        "name": "Access-Token",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "Authorized access token. For details, see Authentication ."
      }
    ],
    "parameters": [
      {
        "name": "bc_id",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "Business Center ID. To get a list of Business Centers that you have access to, use /bc/get/ ."
      }
    ],
    "examples": [
      {
        "title": "Example",
        "curl": "curl --location --request GET 'https://business-api.tiktok.com/open_api/v1.3/bc/advertiser/attribute/?bc_id={{bc_id}}' \\\n--header 'Access-Token: {{Access-Token}}' \\"
      }
    ]
  },
  "response": {
    "example": {
      "status_line": "HTTPS/1.1 200 OK",
      "body": "{\n    \"code\": 0,\n    \"message\": \"OK\",\n    \"request_id\": \"{{request_id}}\",\n    \"data\": {\n        \"region_codes\": [\n            \"AU\",\n            \"AE\",\n            \"AF\",\n            \"JP\",\n            \"BR\",\n            \"US\",\n            \"AD\",\n            \"FR\"\n        ],\n        \"currencies\": [\n            \"USD\"\n        ]\n    }\n}"
    }
  }
}