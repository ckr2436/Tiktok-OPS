{
  "title": "Cancel the sharing of assets",
  "endpoint": "https://business-api.tiktok.com/open_api/v1.3/bc/partner/asset/delete/",
  "method": "POST",
  "category_path": [
    "BC Partners"
  ],
  "official_only": true,
  "request": {
    "headers": [
      {
        "name": "Access-Token",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "Authorized access token. For details, see Authentication ."
      },
      {
        "name": "Content-Type",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "The content type of the request. Allowed value: \"application/json\" ."
      }
    ],
    "parameters": [
      {
        "name": "bc_id",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "Business Center ID"
      },
      {
        "name": "partner_id",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "Partner Business Center ID"
      },
      {
        "name": "asset_ids",
        "required": true,
        "type": "array",
        "raw_type": "string[]",
        "description": "List of asset IDs that you want to cancel the sharing of. Currently, only one form library ID is allowed."
      },
      {
        "name": "asset_type",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "Asset type. Enum values: ADVERTISER , CATALOG , TIKTOK_SHOP , STOREFRANT , and PIXEL ."
      }
    ],
    "examples": [
      {
        "title": "Example",
        "curl": "curl -H \"Access-Token:xxx\" -H \"Content-Type:application/json\" -X POST \\\n-d '{\n    \"bc_id\": \"BC_ID\",\n    \"partner_id\": \"PARTNER_ID\",\n    \"asset_ids\": [\n        \"ASSET_IDS\"\n    ],\n    \"asset_type\": \"ASSET_TYPE\"\n}' \\\nhttps://business-api.tiktok.com/open_api/v1.3/bc/partner/asset/delete/"
      }
    ]
  },
  "response": {
    "example": {
      "status_line": "HTTPS/1.1 200 OK",
      "body": {
        "message": "OK",
        "code": 0,
        "data": {}
      }
    }
  }
}