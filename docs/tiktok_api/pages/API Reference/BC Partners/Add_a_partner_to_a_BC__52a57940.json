{
  "title": "Add a partner to a BC",
  "endpoint": "https://business-api.tiktok.com/open_api/v1.3/bc/partner/add/",
  "method": "POST",
  "category_path": [
    "BC Partners"
  ],
  "official_only": true,
  "request": {
    "headers": [
      {
        "name": "Access-Token",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "Authorized access token. For details, see Authentication ."
      },
      {
        "name": "Content-Type",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "The content type of the request. Allowed value: \"application/json\" ."
      }
    ],
    "parameters": [
      {
        "name": "bc_id",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "Business Center ID"
      },
      {
        "name": "partner_id",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "Partner Business Center ID"
      },
      {
        "name": "asset_type",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "Asset type. Enum value: ADVERTISER ."
      },
      {
        "name": "asset_ids",
        "required": false,
        "type": "array",
        "raw_type": "string[]",
        "description": "List of asset IDs. The assets need to be in the same type."
      },
      {
        "name": "advertiser_role",
        "required": false,
        "type": "string",
        "raw_type": "string",
        "description": "Ad account role assigned to the partner. Valid only when asset_type is ADVERTISER . Enum values: ADMIN , OPERATOR , ANALYST . Default value: ANALYST ."
      }
    ],
    "examples": [
      {
        "title": "Example",
        "curl": "curl -H \"Access-Token:xxx\" -H \"Content-Type:application/json\" -X POST \\\n-d '{\n    \"bc_id\": \"BC_ID\",\n    \"partner_id\": \"PARTNER_ID\",\n    \"asset_type\": \"ASSET_TYPE\",\n    \"asset_ids\": [\n        \"ASSET_IDS\"\n    ],\n    \"advertiser_role\": \"ADVERTISER_ROLE\",\n    \"catalog_role\": \"CATALOG_ROLE\"\n}' \\\nhttps://business-api.tiktok.com/open_api/v1.3/bc/partner/add/"
      }
    ]
  },
  "response": {
    "example": {
      "status_line": "HTTPS/1.1 200 OK",
      "body": {
        "message": "OK",
        "code": 0,
        "data": {}
      }
    }
  }
}