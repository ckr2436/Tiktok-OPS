{
  "title": "Upload an audience file",
  "endpoint": "https://business-api.tiktok.com/open_api/v1.3/dmp/custom_audience/file/upload/",
  "method": "POST",
  "category_path": [
    "Audience",
    "Customer File"
  ],
  "official_only": true,
  "request": {
    "headers": [
      {
        "name": "Access-Token",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "Authorized access token. For details, see Authentication ."
      },
      {
        "name": "Content-Type",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "multipart/form-data"
      }
    ],
    "parameters": [
      {
        "name": "advertiser_id",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "Advertiser ID."
      },
      {
        "name": "file",
        "required": true,
        "type": "string",
        "raw_type": "file",
        "description": "Data files. Only supports CSV and TXT files. The file suffixes should exactly be .csv or .txt ."
      },
      {
        "name": "file_signature",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "The file's MD5, which is used for server-side verification."
      },
      {
        "name": "calculate_type",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "Encryption type. The value for this field must be consistent with the actual file data. Otherwise, your upload will fail or you will not be able to create a valid audience. For enum values, see Enumeration - Encryption Type ."
      }
    ],
    "examples": [
      {
        "title": "Example",
        "curl": "curl --location 'https://business-api.tiktok.com/open_api/v1.3/dmp/custom_audience/file/upload/' \\\n--header 'Access-Token: {{Access-Token}}' \\\n--header 'Content-Type: multipart/form-data' \\\n--form 'file=@\"/path/to/your/file\"' \\\n--form 'advertiser_id=\"{{advertiser_id}}\"' \\\n--form 'file_signature=\"{{file_signature}}\"' \\\n--form 'calculate_type=\"{{calculate_type}}\"'"
      }
    ]
  },
  "response": {
    "example": {
      "status_line": "HTTPS/1.1 200 OK",
      "body": {
        "message": "OK",
        "code": 0,
        "data": {
          "file_path": "f19fa450136c460f"
        },
        "request_id": "11111111111"
      }
    }
  }
}