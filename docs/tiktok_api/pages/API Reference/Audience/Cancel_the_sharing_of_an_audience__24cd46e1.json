{
  "title": "Cancel the sharing of an audience",
  "endpoint": "https://business-api.tiktok.com/open_api/v1.3/dmp/custom_audience/share/cancel/",
  "method": "POST",
  "category_path": [
    "Audience"
  ],
  "official_only": true,
  "request": {
    "headers": [
      {
        "name": "Access-Token",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "Authorized access token. For details, see Authentication ."
      },
      {
        "name": "Content-Type",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "The content type of the request. Allowed value: \"application/json\" ."
      }
    ],
    "parameters": [
      {
        "name": "custom_audience_id",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "Custom audiences that you want to stop sharing."
      },
      {
        "name": "shared_advertiser_id Optional",
        "required": false,
        "type": "string",
        "raw_type": "string",
        "description": "Advertisers that you want to stop sharing audiences with. Only set it when you want to cancel the audiences you shared with others before."
      },
      {
        "name": "advertiser_id",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "Operator advertiser ID."
      }
    ],
    "examples": [
      {
        "title": "Examples",
        "curl": "curl -H \"Access-Token:xxx\" -H \"Content-Type:application/json\" -X POST \\\n-d '{\n    \"custom_audience_id\": \"CUSTOM_AUDIENCE_ID\",\n    \"shared_advertiser_id\": \"SHARED_ADVERTISER_ID\",\n    \"advertiser_id\": \"ADVERTISER_ID\"\n}' \\\nhttps://business-api.tiktok.com/open_api/v1.3/dmp/custom_audience/share/cancel/"
      }
    ]
  },
  "response": {
    "example": {
      "status_line": "HTTPS/1.1 200 OK",
      "body": {
        "message": "OK",
        "code": 0,
        "data": {}
      }
    }
  }
}