{
  "title": "Share audiences",
  "endpoint": "https://business-api.tiktok.com/open_api/v1.3/dmp/custom_audience/share/",
  "method": "POST",
  "category_path": [
    "Audience"
  ],
  "official_only": true,
  "request": {
    "headers": [
      {
        "name": "Access-Token",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "Authorized access token. For details, see Authentication ."
      },
      {
        "name": "Content-Type",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "The content type of the request. Allowed value: \"application/json\" ."
      }
    ],
    "parameters": [
      {
        "name": "custom_audience_ids",
        "required": true,
        "type": "array",
        "raw_type": "string[]",
        "description": "Custom audiences that you want to share. Size: 1-10."
      },
      {
        "name": "shared_advertiser_ids",
        "required": true,
        "type": "array",
        "raw_type": "string[]",
        "description": "Advertisers that you want to share audiences with. They must be in the same Business Center as you. Size: 1-10."
      },
      {
        "name": "advertiser_id",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "Operator advertiser ID."
      }
    ],
    "examples": [
      {
        "title": "Examples",
        "curl": "curl -H \"Access-Token:xxx\" -H \"Content-Type:application/json\" -X POST \\\n-d '{\n    \"custom_audience_ids\": [\n        \"CUSTOM_AUDIENCE_IDS\"\n    ],\n    \"shared_advertiser_ids\": [\n        \"SHARED_ADVERTISER_IDS\"\n    ],\n    \"advertiser_id\": \"ADVERTISER_ID\"\n}' \\\nhttps://business-api.tiktok.com/open_api/v1.3/dmp/custom_audience/share/"
      }
    ]
  },
  "response": {
    "example": {
      "status_line": "HTTPS/1.1 200 OK",
      "body": {
        "message": "OK",
        "code": 0,
        "data": {}
      }
    }
  }
}