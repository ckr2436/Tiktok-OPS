{
  "title": "Get details of audience overlap",
  "endpoint": "https://business-api.tiktok.com/open_api/v1.3/audience/insight/overlap/",
  "method": "GET",
  "category_path": [
    "Audience",
    "Audience Insights"
  ],
  "official_only": true,
  "request": {
    "headers": [
      {
        "name": "Access-Token",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "Authorized access token. For details, see Authentication ."
      }
    ],
    "parameters": [
      {
        "name": "advertiser_id",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "Advertiser ID."
      },
      {
        "name": "benchmark_custom_audience_id",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "The ID of the custom audience that you select as the benchmark audience. Note : The audience ID that you input here must be accessible by your advertiser ID. Otherwise, an error will occur."
      },
      {
        "name": "comparison_custom_audience_ids",
        "required": false,
        "type": "array",
        "raw_type": "string[]",
        "description": "The IDs of the custom audience that you select as the comparison audience. Max size: 4. Note : The audience IDs that you input here must be accessible by your advertiser ID. Otherwise, an error will occur."
      }
    ],
    "examples": [
      {
        "title": "Example",
        "curl": "curl --location --request GET 'https://business-api.tiktok.com/open_api/v1.3/audience/insight/overlap/' \\\n--header 'Content-Type: application/json' \\\n--header 'Access-Token: {{Access-Token}}' \\\n--data '{\n    \"advertiser_id\": \"{{advertiser_id}}\",\n    \"benchmark_custom_audience_id\": \"{{benchmark_custom_audience_id}}\",\n    \"comparison_custom_audience_ids\": [\n        \"{{comparison_custom_audience_ids}}\",\n        \"{{comparison_custom_audience_ids}}\",\n        \"{{comparison_custom_audience_ids}}\",\n        \"{{comparison_custom_audience_ids}}\"\n    ]\n}'"
      }
    ]
  },
  "response": {
    "example": {
      "status_line": "",
      "body": ""
    }
  }
}