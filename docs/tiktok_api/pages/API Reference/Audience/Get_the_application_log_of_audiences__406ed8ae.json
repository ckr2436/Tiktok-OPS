{
  "title": "Get the application log of audiences",
  "endpoint": "https://business-api.tiktok.com/open_api/v1.3/dmp/custom_audience/apply/log/",
  "method": "GET",
  "category_path": [
    "Audience"
  ],
  "official_only": true,
  "request": {
    "headers": [
      {
        "name": "Access-Token",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "Authorized access token. For details, see Authentication ."
      }
    ],
    "parameters": [
      {
        "name": "advertiser_id",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "Advertiser ID."
      },
      {
        "name": "custom_audience_ids",
        "required": true,
        "type": "array",
        "raw_type": "string[]",
        "description": "A list of custom audience IDs. Note The advertiser must be the owner of the custom_audience_ids . Otherwise, an error will occur. You can use the is_creator field returned in /dmp/custom_audience/list/ and /dmp/custom_audience/get/ to check whether this advertiser is the owner of this audience."
      },
      {
        "name": "page",
        "required": false,
        "type": "number",
        "raw_type": "number",
        "description": "Current page number. Default value: 1."
      },
      {
        "name": "page_size",
        "required": false,
        "type": "number",
        "raw_type": "number",
        "description": "Page size. Default value: 10."
      },
      {
        "name": "timezone",
        "required": false,
        "type": "string",
        "raw_type": "string",
        "description": "The timezone of returned value. For enum values, see Appendix - Time Zone . If not specified, UTC will be used."
      }
    ],
    "examples": []
  },
  "response": {
    "example": {
      "status_line": "HTTPS/1.1 200 OK",
      "body": {
        "code": 0,
        "message": "OK",
        "request_id": "123",
        "data": {
          "advertiser_id": "123",
          "page_info": {
            "total_number": 8,
            "page_size": 10,
            "page": 1,
            "total_page": 1
          },
          "list": [
            {
              "audience_id": "123",
              "adgroup_name": "123",
              "action_timestamp": "2022-06-15 11:31:00",
              "editor": "123",
              "usage_mode": "Include",
              "adgroup_id": "123"
            },
            {
              "audience_id": "123",
              "adgroup_name": "123",
              "action_timestamp": "2023-01-23 22:49:06",
              "editor": "123",
              "usage_mode": "Exclude",
              "adgroup_id": "123"
            }
          ]
        }
      }
    }
  }
}