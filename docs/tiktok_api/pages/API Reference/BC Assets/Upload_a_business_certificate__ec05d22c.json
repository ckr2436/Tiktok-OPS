{
  "title": "Upload a business certificate",
  "endpoint": "https://business-api.tiktok.com/open_api/v1.3/bc/image/upload/",
  "method": "POST",
  "category_path": [
    "BC Assets"
  ],
  "official_only": true,
  "request": {
    "headers": [
      {
        "name": "Access-Token",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "Authorized access token. For details, see Authentication ."
      },
      {
        "name": "Content-Type",
        "required": true,
        "type": "string",
        "raw_type": "String",
        "description": "Request message type, upload files in multipart/form-data format."
      }
    ],
    "parameters": [
      {
        "name": "bc_id",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "Business Center ID"
      },
      {
        "name": "image_file",
        "required": true,
        "type": "string",
        "raw_type": "file",
        "description": "The certificate image file to be uploaded. Supported picture format: JPG/JPEG/PNG. The maximum file size is 10 MB."
      }
    ],
    "examples": [
      {
        "title": "Example",
        "curl": "curl --request POST -H \"Access-Token:xxx\" \\\n--form 'bc_id=bc_id' \\\n--form 'image_file=@\"filepath\"' \\\nhttps://business-api.tiktok.com/open_api/v1.3/bc/image/upload/"
      }
    ]
  },
  "response": {
    "example": {
      "status_line": "HTTPS/1.1 200 OK",
      "body": {
        "message": "OK",
        "code": 0,
        "data": {}
      }
    }
  }
}