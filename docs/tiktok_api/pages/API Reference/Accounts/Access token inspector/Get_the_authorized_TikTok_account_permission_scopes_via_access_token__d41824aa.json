{
  "title": "Get the authorized TikTok account permission scopes via access token",
  "endpoint": "https://business-api.tiktok.com/open_api/v1.3/tt_user/token_info/get/",
  "method": "POST",
  "category_path": [
    "Accounts",
    "Access token inspector"
  ],
  "official_only": true,
  "request": {
    "headers": [
      {
        "name": "Content-Type",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "Request message type. Allowed format: application/json ."
      }
    ],
    "parameters": [
      {
        "name": "app_id",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "ID of your developer application. You can find the App ID by navigating to My Apps > App Detail > Basic Information ."
      },
      {
        "name": "access_token",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "Access token authorized by TikTok accounts. For details, see Authentication ."
      }
    ],
    "examples": [
      {
        "title": "Example",
        "curl": "curl --location --request POST 'https://business-api.tiktok.com/open_api/v1.3/tt_user/token_info/get/'\n--header 'Content-Type: application/json'\n--data-raw '{\n    \"app_id\":\"{{app_id}}\",\n    \"access_token\":\"{{access_token}}\"\n}'"
      }
    ]
  },
  "response": {
    "example": {
      "status_line": "HTTPS/1.1 200 OK",
      "body": "{\n    \"code\": 0,\n    \"message\": \"OK\",\n    \"request_id\": \"{{request_id}}\",\n    \"data\": {\n        \"app_id\": \"{{app_id}}\",\n        \"creator_id\": \"{{creator_id}}\",\n        \"scope\": \"video.publish,user.info.basic,video.list,user.insights,user.insights.creator,tcm.order.update,comment.list.manage,video.insights,comment.list\"\n    }\n}"
    }
  }
}