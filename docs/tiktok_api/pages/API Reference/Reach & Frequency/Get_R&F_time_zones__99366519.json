{
  "title": "Get R&F time zones",
  "endpoint": "https://business-api.tiktok.com/open_api/v1.3/rf/delivery/timezone/",
  "method": "GET",
  "category_path": [
    "Reach & Frequency"
  ],
  "official_only": true,
  "request": {
    "headers": [
      {
        "name": "Access-Token",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "Authorized access token. For details, see Authentication ."
      }
    ],
    "parameters": [
      {
        "name": "advertiser_id",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "Advertiser ID"
      },
      {
        "name": "region_codes",
        "required": true,
        "type": "array",
        "raw_type": "string[]",
        "description": "Country codes to be queried. Example: US . See Appendix - Location Code"
      }
    ],
    "examples": [
      {
        "title": "Example",
        "curl": "curl --get -H \"Access-Token:xxx\" \\\n--data-urlencode \"advertiser_id=ADVERTISER_ID\" \\\n--data-urlencode \"country_codes=[\\\"COUNTRY_CODES\\\"]\" \\\nhttps://business-api.tiktok.com/open_api/v1.3/rf/delivery/timezone/"
      }
    ]
  },
  "response": {
