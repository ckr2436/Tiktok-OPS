{
  "title": "Cancel the R&F ad order",
  "endpoint": "https://business-api.tiktok.com/open_api/v1.3/rf/order/cancel/",
  "method": "POST",
  "category_path": [
    "Reach & Frequency"
  ],
  "official_only": true,
  "request": {
    "headers": [
      {
        "name": "Access-Token",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "Authorized access token. For details, see Authentication ."
      }
    ],
    "parameters": [
      {
        "name": "advertiser_id",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "Advertiser ID"
      },
      {
        "name": "adgroup_ids",
        "required": true,
        "type": "array",
        "raw_type": "string[]",
        "description": "ID of the ad group to be withdrawn (ID must correspond to R&F ad group)."
      }
    ],
    "examples": [
      {
        "title": "Example",
        "curl": "curl -H \"Access-Token:xxx\" -H \"Content-Type:application/json\" -X POST \\\n-d '{\n    \"advertiser_id\": \"ADVERTISER_ID\", \n    \"adgroup_ids\": [\n        \"ADGROUP_IDS\"\n    ]\n}' \\\nhttps://business-api.tiktok.com/open_api/v1.3/rf/order/cancel/"
      }
    ]
  },
  "response": {
    "example": {
      "status_line": "HTTPS/1.1 200 OK",
      "body": "{\n    \"message\": \"OK\",\n    \"code\": 0,\n    \"data\": {\n        \"fail_adgroup_ids\": [222]\n    }\n    \"request_id\":\"12345678\"\n}"
    }
  }
}