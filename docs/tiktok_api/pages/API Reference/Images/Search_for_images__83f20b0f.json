{
  "title": "Search for images",
  "endpoint": "https://business-api.tiktok.com/open_api/v1.3/file/image/ad/search/",
  "method": "GET",
  "category_path": [
    "Images"
  ],
  "official_only": true,
  "request": {
    "headers": [
      {
        "name": "Access-Token",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "Authorized access token. For details, see Authentication ."
      }
    ],
    "parameters": [
      {
        "name": "advertiser_id",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "Advertiser ID."
      },
      {
        "name": "filtering",
        "required": false,
        "type": "object",
        "raw_type": "object",
        "description": "Filters on the data. This parameter is an array of filter objects."
      },
      {
        "name": "height",
        "required": false,
        "type": "number",
        "raw_type": "number",
        "description": "Image height."
      },
      {
        "name": "image_ids",
        "required": false,
        "type": "array",
        "raw_type": "string[]",
        "description": "Image IDs. At most 100 IDs can be included in the list."
      },
      {
        "name": "material_ids",
        "required": false,
        "type": "array",
        "raw_type": "string[]",
        "description": "A list of material IDs. At most 100 IDs can be included in the list."
      },
      {
        "name": "ratio",
        "required": false,
        "type": "number",
        "raw_type": "float[]",
        "description": "Image aspect ratio, e.g.: [1.7, 2.5]. Use 1.7 to search for Images with aspect ratio between 1.65-1.75, i.e. the precision floating point is 0.05."
      },
      {
        "name": "width",
        "required": false,
        "type": "number",
        "raw_type": "number",
        "description": "Image width."
      },
      {
        "name": "displayable",
        "required": false,
        "type": "boolean",
        "raw_type": "boolean",
        "description": "Enum values: False (default value): Search in all materials. True : Search in the materials displayed on the platform."
      },
      {
        "name": "page",
        "required": false,
        "type": "number",
        "raw_type": "number",
        "description": "Current page number. Default value: 1. Value range: â‰¥ 1."
      },
      {
        "name": "page_size",
        "required": false,
        "type": "number",
        "raw_type": "number",
        "description": "Page size. Default value: 20. Value range: 1-100."
      }
    ],
    "examples": [
      {
        "title": "Example",
        "curl": "curl --location --request GET 'https://business-api.tiktok.com/open_api/v1.3/file/image/ad/search/?advertiser_id={{advertiser_id}}' \\\n--header 'Access-Token: {{Access-Token}}'"
      }
    ]
  },
  "response": {
    "example": {
      "status_line": "HTTPS/1.1 200 OK",
      "body": "{\n    \"code\": 0,\n    \"message\": \"OK\",\n    \"request_id\": \"{{request_id}}\",\n    \"data\": {\n        \"page_info\": {\n            \"page_size\": 20,\n            \"total_page\": 1,\n            \"total_number\": 2,\n            \"page\": 1\n        },\n        \"list\": [\n            {\n                \"height\": 720,\n                \"size\": 118834,\n                \"image_url\": \"{{image_url}}\",\n                \"modify_time\": \"{{modify_time}}\",\n                \"create_time\": \"{{create_time}}\",\n                \"image_id\": \"{{image_id}}\",\n                \"file_name\": \"{{file_name}}\",\n                \"width\": 1280,\n                \"signature\": \"{{signature}}\",\n                \"displayable\": false,\n                \"material_id\": \"{{material_id}}\",\n                \"format\": \"jpeg\",\n                \"is_carousel_usable\": false\n            },\n            {\n                \"height\": 720,\n                \"size\": 159992,\n                \"image_url\": \"{{image_url}}\",\n                \"modify_time\": \"{{modify_time}}\",\n                \"create_time\": \"{{create_time}}\",\n                \"image_id\": \"{{image_id}}\",\n                \"file_name\": \"{{file_name}}\",\n                \"width\": 1280,\n                \"signature\": \"{{signature}}\",\n                \"displayable\": false,\n                \"material_id\": \"{{material_id}}\",\n                \"format\": \"jpeg\",\n                \"is_carousel_usable\": false\n            }\n        ]\n    }\n}"
    }
  }
}