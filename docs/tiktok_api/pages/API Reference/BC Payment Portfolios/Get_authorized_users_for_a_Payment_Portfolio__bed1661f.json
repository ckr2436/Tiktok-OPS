{
  "title": "Get authorized users for a Payment Portfolio",
  "endpoint": "https://business-api.tiktok.com/open_api/v1.3/payment_portfolio/user/get/",
  "method": "GET",
  "category_path": [
    "BC Payment Portfolios"
  ],
  "official_only": true,
  "request": {
    "headers": [
      {
        "name": "Access-Token",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "Authorized access token. For details, see Authentication ."
      }
    ],
    "parameters": [
      {
        "name": "payment_portfolio_id",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "The ID of the Payment Portfolio. To obtain a list of Payment Portfolio IDs, use /payment_portfolio/get/ ."
      },
      {
        "name": "page",
        "required": false,
        "type": "integer",
        "raw_type": "integer",
        "description": "Current page number. Default value: 1. Value range: â‰¥ 1."
      },
      {
        "name": "page_size",
        "required": false,
        "type": "integer",
        "raw_type": "integer",
        "description": "Page size. Default value: 10. Value range: 1-50."
      }
    ],
    "examples": [
      {
        "title": "Example",
        "curl": "curl --location --request GET 'https://business-api.tiktok.com/open_api/v1.3/payment_portfolio/user/get/?payment_portfolio_id={{payment_portfolio_id}}&page=1&page_size=50' \\\n--header 'Access-Token: {{Access-Token}}'"
      }
    ]
  },
  "response": {
    "example": {
      "status_line": "HTTPS/1.1 200 OK",
      "body": "{\n    \"code\": 0,\n    \"message\": \"OK\",\n    \"request_id\": \"{{request_id}}\",\n    \"data\": {\n        \"page_info\": {\n            \"page\": 1,\n            \"page_size\": 50,\n            \"total_number\": 2,\n            \"total_page\": 1\n        },\n        \"users\": [\n            {\n                \"user_id\": \"{{user_id}}\",\n                \"user_name\": \"{{user_name}}\"\n            },\n            {\n                \"user_id\": \"{{user_id}}\",\n                \"user_name\": \"{{user_name}}\"\n            }\n        ]\n    }\n}"
    }
  }
}