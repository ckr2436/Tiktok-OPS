{
  "title": "Get Payment Portfolios",
  "endpoint": "https://business-api.tiktok.com/open_api/v1.3/payment_portfolio/get/",
  "method": "GET",
  "category_path": [
    "BC Payment Portfolios"
  ],
  "official_only": true,
  "request": {
    "headers": [
      {
        "name": "Access-Token",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "Authorized access token. For details, see Authentication ."
      }
    ],
    "parameters": [
      {
        "name": "bc_id",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "ID of a Business Center. By default, the response will include the Payment Portfolios that belong to the same client as the Business Center, even though some of them are not linked to the Business Center."
      },
      {
        "name": "filtering",
        "required": false,
        "type": "object",
        "raw_type": "object",
        "description": "Filtering conditions."
      },
      {
        "name": "payment_portfolio_ids",
        "required": false,
        "type": "array",
        "raw_type": "string[]",
        "description": "IDs of the Payment Portfolios to filter the results by. Max size: 10. Invalid Payment Portfolio IDs, once specified, will be ignored."
      },
      {
        "name": "bc_related",
        "required": false,
        "type": "boolean",
        "raw_type": "boolean",
        "description": "Whether to only return the Payment Portfolios that are linked to the Business Center. Supported values: true , false ."
      },
      {
        "name": "page",
        "required": false,
        "type": "integer",
        "raw_type": "integer",
        "description": "Current page number. Default value: 1. Value range: â‰¥ 1."
      },
      {
        "name": "page_size",
        "required": false,
        "type": "integer",
        "raw_type": "integer",
        "description": "Page size. Default value: 10. Value range: 1-50."
      }
    ],
    "examples": [
      {
        "title": "Example",
        "curl": "curl --location --request GET 'https://business-api.tiktok.com/open_api/v1.3/payment_portfolio/get/?bc_id={{bc_id}}&page=1&page_size=50' \\\n--header 'Access-Token: {{Access-Token}}'"
      }
    ]
  },
  "response": {
    "example": {
      "status_line": "HTTPS/1.1 200 OK",
      "body": "{\n    \"code\": 0,\n    \"message\": \"OK\",\n    \"request_id\": \"{{request_id}}\",\n    \"data\": {\n        \"page_info\": {\n            \"page\": 1,\n            \"page_size\": 50,\n            \"total_number\": 2,\n            \"total_page\": 1\n        },\n        \"payment_portfolios\": [\n            {\n                \"cash_balance_list\": [\n                    {\n                        \"amount\": 0,\n                        \"currency\": \"USD\"\n                    }\n                ],\n                \"credit_line_allocation\": {\n                    \"allocated_amount\": 0,\n                    \"available_amount\": 0,\n                    \"min_allocation_percentage\": 0,\n                    \"used_amount\": 0\n                },\n                \"linked_advertiser_account_count\": 0,\n                \"payment_portfolio_id\": \"{{payment_portfolio_id}}\",\n                \"payment_portfolio_name\": \"{{payment_portfolio_name}}\",\n                \"user_count\": 7\n            },\n            {\n                \"cash_balance_list\": [\n                    {\n                        \"amount\": 0,\n                        \"currency\": \"USD\"\n                    }\n                ],\n                \"credit_line_allocation\": {\n                    \"allocated_amount\": 0,\n                    \"available_amount\": 0,\n                    \"min_allocation_percentage\": 0,\n                    \"used_amount\": 0\n                },\n                \"linked_advertiser_account_count\": 2,\n                \"payment_portfolio_id\": \"{{payment_portfolio_id}}\",\n                \"payment_portfolio_name\": \"{{payment_portfolio_name}}\",\n                \"user_count\": 7\n            }\n        ]\n    }\n}"
    }
  }
}