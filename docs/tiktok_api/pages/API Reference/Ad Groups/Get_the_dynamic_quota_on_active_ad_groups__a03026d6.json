{
  "title": "Get the dynamic quota on active ad groups",
  "endpoint": "https://business-api.tiktok.com/open_api/v1.3/adgroup/quota/",
  "method": "GET",
  "category_path": [
    "Ad Groups"
  ],
  "official_only": true,
  "request": {
    "headers": [
      {
        "name": "Access-Token",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "Authorized access token. For details, see Authentication ."
      }
    ],
    "parameters": [
      {
        "name": "advertiser_id",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "Advertiser ID."
      }
    ],
    "examples": [
      {
        "title": "Example",
        "curl": "curl --location --request GET 'https://business-api.tiktok.com/open_api/v1.3/adgroup/quota/?advertiser_id={{advertiser_id}}' \\\n--header 'Access-Token: {{Access-Token}}'"
      }
    ]
  },
  "response": {
    "example": {
      "status_line": "HTTPS/1.1 200 OK",
      "body": "{\n    \"code\": 0,\n    \"message\": \"OK\",\n    \"request_id\": \"{{request_id}}\",\n    \"data\": {\n        \"total_adgroup_quota\": 4980,\n        \"used_adgroup_quota\": 20\n    }\n}"
    }
  }
}