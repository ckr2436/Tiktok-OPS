{
  "title": "Get authorized ad accounts",
  "endpoint": "https://business-api.tiktok.com/open_api/v1.3/oauth2/advertiser/get/",
  "method": "GET",
  "category_path": [
    "Ad Accounts"
  ],
  "official_only": true,
  "request": {
    "headers": [
      {
        "name": "Access-Token",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "Authorized access token. For details, see Authentication ."
      }
    ],
    "parameters": [
      {
        "name": "app_id",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "The App id applied by the developer, which can be found on the Application Management page."
      },
      {
        "name": "secret",
        "required": true,
        "type": "string",
        "raw_type": "string",
        "description": "The private key of the developer's application, which can be found on the Application Management page."
      }
    ],
    "examples": [
      {
        "title": "Example",
        "curl": "curl --get -H \"Access-Token:<access_token>\" \\\n--data-urlencode \"secret=<secret>\" \\\n--data-urlencode \"app_id=<app_id>\" \\\nhttps://business-api.tiktok.com/open_api/v1.3/oauth2/advertiser/get/"
      }
    ]
  },
  "response": {
    "example": {
      "status_line": "HTTPS/1.1 200 OK",
      "body": {
        "message": "OK",
        "code": 0,
        "data": {
          "list": [
            {
              "advertiser_id": "ADVERTISER_ID",
              "advertiser_name": "ADVERTISER_NAME"
            }
          ]
        },
        "request_id": "202003100820230101890492231E063EE3"
      }
    }
  }
